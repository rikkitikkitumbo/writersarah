WriterSarah.config ["$stateProvider", "$urlRouterProvider", ($stateProvider, $urlRouterProvider) ->

    $urlRouterProvider
    .when('', '/category/Press')
    .otherwise('/category/Press')

    $stateProvider

    .state('app' ,
        url: '/'
        templateUrl: 'assets/cats/cat_index.html.slim'
        controller: 'PublicController'
    )
    .state('app.show',
        url: "category/:title"
        templateUrl: '/assets/cats/cat_show.html.slim'
        controller: 'PublicController'
    )




    .state('edit' ,
        url: '/'
        templateUrl: '/assets/cats/edit/edit_cat_index.html.slim'
        controller: 'CatsController'
    )
    .state('edit.show',
        url: "edit/category/:title"
        templateUrl: '/assets/cats/edit/edit_cat_show.html.slim'
        controller: 'CatsController'
    )

]